{
  "name": "kei.gas.calFactory",
  "version": "0.0.4",
  "type": "module",
  "exports": "./build/src/main.ts",
  "main": "./build/src/main.ts",
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "scripts": {
    "login": "clasp login",
    "push": "clasp push",
    "build": "npm run compile && npm i -g --loglevel=error --force",
    "build-fresh": "npm cache clean --force && npm i && npm run build",
    "watch": "tsc --project tsconfig.json --watch",
    "prepare": "npm run compile",
    "lint": "npm run check",
    "test": "jest --cache false",
    "prettier": "prettier src test --write",
    "check": "gts check src/*.ts src/**/*.ts",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix src/*.ts src/**/*.ts",
    "pretest": "npm run compile",
    "doc": "jsdoc -c ./jsdoc.json",
    "mddoc": "jsdoc2md --files ./src/*.ts --configure ./jsdoc2md.json> ./md_doc/README.md"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nominalrune/kei.gas.calFactory"
  },
  "keywords": [
    "Apps",
    "Script",
    "SDK",
    "API",
    "script.google.com",
    "extension",
    "add-on"
  ],
  "dependencies": {
    "@sindresorhus/is": "^4.0.1",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.2",
    "cli-truncate": "^3.0.0",
    "commander": "^8.1.0",
    "debounce": "^1.2.1",
    "dotf": "^2.0.2",
    "find-up": "^6.0.0",
    "fs-extra": "^10.0.0",
    "fuzzy": "^0.1.3",
    "google-auth-library": "^7.6.2",
    "googleapis": "^84.0.0",
    "inquirer": "^8.1.2",
    "inquirer-autocomplete-prompt-ipt": "^2.0.0",
    "is-reachable": "^5.0.0",
    "log-symbols": "^5.0.0",
    "loud-rejection": "^2.2.0",
    "make-dir": "^3.1.0",
    "multimatch": "^5.0.0",
    "normalize-newline": "^4.1.0",
    "open": "^8.2.1",
    "ora": "^6.0.0",
    "p-map": "^5.1.0",
    "read-pkg-up": "^8.0.0",
    "recursive-readdir": "^2.2.2",
    "server-destroy": "^1.0.1",
    "split-lines": "^3.0.0",
    "strip-bom": "^5.0.0",
    "ts2gas": "^4.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/chai": "^4.2.21",
    "@types/debounce": "^1.2.0",
    "@types/fs-extra": "^9.0.12",
    "@types/google-apps-script": "^1.0.45",
    "@types/inquirer": "^7.3.3",
    "@types/jest": "^27.4.1",
    "@types/mocha": "^9.1.0",
    "@types/node": "^12.20.21",
    "@types/recursive-readdir": "^2.2.0",
    "@types/server-destroy": "^1.0.1",
    "@types/tmp": "^0.2.1",
    "better-docs": "^2.7.2",
    "chai": "^4.3.4",
    "coveralls": "^3.1.1",
    "gts": "^3.1.0",
    "jest": "^27.5.1",
    "jsdoc-babel": "^0.5.0",
    "jsdoc-to-markdown": "^7.1.1",
    "nyc": "^15.1.0",
    "prettier": "^2.3.2",
    "tmp": "^0.2.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "type-fest": "^2.1.0",
    "typescript": "^4.5.5"
  }
}
